{
    /* SydneyHUD */
    "language": 1,

    /* Menu Tweak */
    "lobby_skins_mode": 1,
    
    "enable_buy_all_assets": true,

    "skip_black_screen": true,

    "skip_stat_screen": true,
    "skip_stat_screen_delay": 0,

    "skip_card_pick": true,

    "skip_loot_screen": true,
    "skip_loot_screen_delay": 0,

    /* HUD Tweak */
    "enable_pacified": true,
    "center_assault_banner": true,
    "enable_enhanced_assault_banner": true,

    "show_interaction_circle": true,
    "show_interaction_text": true,

    "show_text_borders": true,

    "enable_objective_animation": true,
    
    "show_suspicion_text": true,

    "show_reload_interaction": true,
    "show_melee_interaction": true,

    "anti_bobble": true,

    "interaction_color_r": 1,
    "interaction_color_g": 0.66,
    "interaction_color_b": 0,

    /* Interact Tweak */
    "hold_to_pick": true,
    "hokd_to_pick_delay": 0.1,

    "push_to_interact": true,
    "equipment_interrupt": true,

    /* Gadget Tweak */
    "enable_flashlight_extender": true,
    "flashlight_range": 1000,
    "flashlight_angle": 60,

    "enable_laser_options": true,
    "laser_color_r": 0,
    "laser_color_g": 1,
    "laser_color_b": 0,

    "laser_color_rainbow": true,

    "laser_light": 10,
    "laser_glow": 1,

    "laser_color_a": 0.5,

    /* GamePlay Tweak */
    "anti_stealth_grenades": true
}